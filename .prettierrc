{
  "printWidth": 140,
  "tabWidth": 2,
  "trailingComma": "none",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "semi": false,
  "singleAttributePerLine": false,
  "importOrder": [
    "^next/(.*)$",
    "^react(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/public/(.*)$",
    "^@/store(.*)$",
    "^@/redux/(.*)$",
    "^@/api_slices/(.*)$",
    "^@/utils/(.*)$",
    "^@/hooks/(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}